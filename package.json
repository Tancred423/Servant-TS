{
  "name": "servant-v8",
  "version": "8.0.0",
  "description": "A multifunctional Discord Bot",
  "main": "dist/app.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "start": "node dist/app.js",
    "watch:build": "tsc --watch",
    "watch:app": "nodemon './dist/app.js' --watch './dist'",
    "watch:start": "npm-run-all clean build --parallel watch:build watch:app --print-label"
  },
  "keywords": [
    "bot",
    "servant",
    "discord"
  ],
  "author": "Tancred#0001",
  "license": "ISC",
  "dependencies": {
    "@discordjs/builders": "^0.13.0",
    "@discordjs/rest": "^0.4.1",
    "@types/node": "^17.0.35",
    "discord-api-types": "^0.33.0",
    "discord.js": "^13.7.0",
    "dotenv": "^16.0.1",
    "moment": "^2.29.3",
    "mysql2": "^2.3.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "@types/mysql2": "github:types/mysql2",
    "ts-node": "^10.7.0"
  }
}
